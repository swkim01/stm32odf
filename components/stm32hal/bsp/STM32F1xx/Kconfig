if ARCH_STM32F1

choice
    prompt "MCU"
    default ARCH_STM32F103RB

config ARCH_STM32F103RB
    bool "STM32F103RB 128KB"
    select FLASH_SIZE_128KB
    select RAM_SIZE_20KB
    select ARCH_STM32F103
config ARCH_STM32F103C6
    bool "STM32F103C6 32KB"
    select FLASH_SIZE_32KB
    select RAM_SIZE_10KB
    select ARCH_STM32F103
config ARCH_STM32F103C8
    bool "STM32F103C8 64KB"
    select FLASH_SIZE_64KB
    select RAM_SIZE_20KB
    select ARCH_STM32F103
config ARCH_STM32F103CB
    bool "STM32F103CB 128KB"
    select FLASH_SIZE_128KB
    select RAM_SIZE_20KB
    select ARCH_STM32F103

endchoice

endif

