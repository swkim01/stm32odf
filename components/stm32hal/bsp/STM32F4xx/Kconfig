if ARCH_STM32F4

choice
    prompt "MCU"
    default ARCH_STM32F446RE

config ARCH_STM32F446RE
    bool "STM32F446RE 512KB"
    select FLASH_SIZE_512KB
    select RAM_SIZE_128KB
    select ARCH_STM32F446
config ARCH_STM32F401RE
    bool "STM32F401RE 512KB"
    select FLASH_SIZE_512KB
    select RAM_SIZE_96KB
    select ARCH_STM32F401
config ARCH_STM32F429ZI
    bool "STM32F429ZI 2MB"
    select FLASH_SIZE_2MB
    select RAM_SIZE_256KB
    select ARCH_STM32F429
config ARCH_STM32F411CE
    bool "STM32F411CE 512KB"
    select FLASH_SIZE_512KB
    select RAM_SIZE_128KB
    select ARCH_STM32F411

endchoice

endif

